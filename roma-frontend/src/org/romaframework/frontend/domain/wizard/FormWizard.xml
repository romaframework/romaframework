<?xml version="1.0" encoding="UTF-8"?>
<class xmlns="http://www.romaframework.org/xml/roma" xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
	xsd:schemaLocation="http://www.romaframework.org/xml/roma http://www.romaframework.org/schema/v3/roma.xsd">
	<aspects>
		<view>
			<form>
				<area name="main" type="grid" size="1" align="top center">
					<area name="content" align="top center" />
					<area name="actions" type="row" align="top center" />
				</area>
			</form>
		</view>
	</aspects>
	<fields>
		<field name="content">
			<aspects>
				<core useRuntimeType="true" />
				<view render="objectembedded" label="" />
				<validation />
			</aspects>
		</field>
		<field name="entity">
			<aspects>
				<view visible="false" />
			</aspects>
		</field>
	</fields>
	<actions>
		<action name="cancel" />
		<action name="begin" />
		<action name="back" />
		<action name="next" />
		<action name="finish" />
	</actions>
</class>
