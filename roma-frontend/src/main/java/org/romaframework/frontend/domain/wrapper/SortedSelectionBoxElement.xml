<?xml version="1.0" encoding="UTF-8"?>
<class xmlns="http://www.romaframework.org/xml/roma" xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
	xsd:schemaLocation="http://www.romaframework.org/xml/roma http://www.romaframework.org/schema/v3/roma.xsd">
	<aspects>
		<view>
			<form>
				<area name="main" size="1" type="grid">
					<area name="filterFields" size="1" type="grid">
						<area name="elementsArea" type="row">
							<area name="elementiDisponibiliArea" type="column">
								<area name="availableElements" size="1" type="grid" />
							</area>
							
							<area name="selectionCommands">
								<area type="row" name="addAll" />
								<area type="row" name="selectElement" />
								<area type="row" name="unselectElement" />
								<area type="row" name="removeAll" />
							</area>
							
							<area name="elementiSelezionateArea" type="column">
								<area name="selectedElements" size="1" type="grid" />
							</area>
							
							<area name="sortingCommands">
								<area name="moveUp" type="row" />
								<area name="moveDown" type="row" />
							</area>
						</area>
					</area>
				</area>
			</form>
		</view>
	</aspects>
</class>
