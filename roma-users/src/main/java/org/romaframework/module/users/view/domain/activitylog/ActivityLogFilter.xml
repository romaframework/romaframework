<?xml version="1.0" encoding="UTF-8"?>
<class xmlns="http://www.romaframework.org/xml/roma" xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
	xsd:schemaLocation="http://www.romaframework.org/xml/roma http://www.romaframework.org/schema/v3/roma.xsd">
	<aspects>
	</aspects>
	<fields>
		<field name="rangeFrom" />
		<field name="rangeTo" />
		<field name="when">
			<aspects>
				<view visible="false" />
			</aspects>
		</field>
		<field name="account">
			<aspects>
				<view enabled="true" />
			</aspects>
		</field>
		<field name="level">
			<aspects>
				<view visible="false" />
			</aspects>
		</field>
		<field name="levels">
			<aspects>
				<view label="$level" render="select" />
			</aspects>
		</field>
		<field name="category">
			<aspects>
				<view visible="false" />
			</aspects>
		</field>
		<field name="categories">
			<aspects>
				<view label="$category" render="select" />
			</aspects>
		</field>
		<field name="notes" />
	</fields>
</class>
